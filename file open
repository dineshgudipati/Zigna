for filename in os.listdir(csv_directory):
    if filename.endswith('.csv'):
        file_path = os.path.join(csv_directory, filename)
        with open(file_path, 'r') as file:
            # Skip the header row if needed
            next(file)
            
            # Get the table name from the CSV file name
            table_name = filename[:-4]  # Remove the '.csv' extension from the filename
            
            # Prepare the SQL INSERT statement
            insert_query = f"INSERT INTO {table_name} (column1, column2, ...) VALUES %s"
            
            # Read the CSV data and convert it into a list of tuples
            data = [tuple(line.strip().split(',')) for line in file]
            
            # Execute the INSERT statement with the data
            cursor.executemany(insert_query, data)
            
        conn.commit()
